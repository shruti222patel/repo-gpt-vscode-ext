{
  "name": "repo-gpt",
  "displayName": "repo-gpt",
  "description": "",
  "version": "0.0.13",
  "publisher": "shruti222patel",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shruti222patel/repo-gpt-vscode-ext.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:repogpt.view",
    "onLanguage:typescript",
    "onLanguage:php",
    "onLanguage:python"
  ],
  "contributes": {
    "languages": [
      {
        "id": "typescript",
        "aliases": [
          "Typescript",
          "typescript",
          "ts"
        ],
        "extensions": [
          ".ts"
        ]
      },
      {
        "id": "php",
        "aliases": [
          "PHP",
          "php"
        ],
        "extensions": [
          ".php"
        ]
      },
      {
        "id": "python",
        "aliases": [
          "Python",
          "py",
          "python"
        ],
        "extensions": [
          ".py"
        ]
      },
      {
        "id": "sql",
        "aliases": [
          "SQL",
          "sql"
        ],
        "extensions": [
          ".sql"
        ]
      }
    ],
    "commands": [
      {
        "command": "repogpt.explain",
        "title": "Explain"
      },
      {
        "command": "repogpt.refactor",
        "title": "Refactor"
      },
      {
        "command": "repogpt.createTest",
        "title": "Create Test"
      }
    ],
    "configuration": {
      "title": "Repo GPT",
      "properties": {
        "repogpt.openaiApiKey": {
          "type": "string",
          "default": null,
          "description": "The OpenAI API key to use for requests."
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "repogpt",
          "title": "RepoGPT",
          "icon": "images/icon.png"
        }
      ]
    },
    "views": {
      "repogpt": [
        {
          "type": "webview",
          "id": "repogpt.view",
          "name": "RepoGPT"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "highlight.js": "^11.8.0"
  }
}
